            <string>///Move &amp; Charge
if (charging &gt; 0)
{
    v_max = charge_speed;
    image_spd = 0;
    charging--;
    if (vx == 0 &amp;&amp; vy == 0)
    {
        charging = 0;
    }
}
else
{
    if (instance_exists(objPlayer))
    {
        var dir = point_direction(x, y, objPlayer.x, objPlayer.y);
        var spd = 0.1;
        add_speed(spd, dir);
        v_max = move_speed;
        if (charge_timer &lt; room_speed)
        {
            //Increase animation speed while getting ready to charge... it's cute, right?
            image_spd = 0.5;
            vx = 0;
            vy = 0;
            if (charge_timer &lt; 0)
            {
                //Charge!
                charge_timer = room_speed*random_range(4, 6);
                spd = charge_speed;
                v_max = charge_speed;
                dir = point_direction(x, y, objPlayer.x, objPlayer.y);
                set_speed(spd, dir);
                charging = room_speed;
            }
        }
        else
        {
            //Go back to normal animation speed after charging
            image_spd = 0.2;
        }
        //Decrease the timer
        charge_timer -= objControl.game_speed;
    }
}


